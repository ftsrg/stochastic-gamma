package voteradapter_p
import "interfaces.gcd"
import "voter_p"
import "gps.gcd"
adapter Voterasync (sensor_num:integer) of component voter : Voter(sensor_num) []{
	when Faults.any / run
	when Sensor.any / run
	queue faultQueue (priority=1,capacity=1) {
		Faults.any
	}
	queue s1Queue (priority=2,capacity=1) {
		Sensor.any
	}
}